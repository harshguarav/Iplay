build/
dist/
node_modules/

# Ignore environment files
.env

# Ignore config files
*.lock
package-lock.json
yarn.lock

# Ignore compiled files
*.min.js
*.min.css

# Ignore test coverage
coverage/

# Optional: ignore JSON files (if needed)
*.json

# Ignore all files in a specific folder
public/*
.vscode/
